<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.demo.mapper.GroupMapper">

    <select id="getGroupList" resultType="GroupDto">
        SELECT t.groups_id, t.member_id FROM test.`groups` t
    </select>

    <select id="getGroupInfo" resultType="GroupDto">
        select t.groups_id, t.member_id from  test.`groups` t where t.groups_id = #{ groupId }
    </select>

</mapper>
