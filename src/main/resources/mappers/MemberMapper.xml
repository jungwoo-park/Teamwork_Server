<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.demo.mapper.MemberMapper">

    <select id="getMemberInfo" resultType="map">
        select * from member where g.member_id= #{memberId};
    </select>

    <select id="searchMemberInfo" resultType="map">
        select * from member where name like CONCAT('%', #{name}, '%');
    </select>

    <select id="insertMemberInfo" resultType="map">

    </select>

</mapper>
